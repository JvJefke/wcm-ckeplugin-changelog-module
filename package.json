{
    "name": "@wcm/ckeplugin-changelog",
    "version": "0.2.0",
    "description": "CKEditor changelog plugin for AM",
    "main": "app",
    "publishConfig": {
        "@wcm:registry": "https://nexusrepo.antwerpen.be/repository/npm-private"
    },
    "keywords": [
        "Plugin",
        "CKEditor",
        "Pelorus",
        "CMS",
        "WCM"
    ],
    "wcmModule": {
        "angularDirPath": "public/app/",
        "assetsDirPath": "public/assets/",
        "cssDirPath": "public/assets/css",
        "moduleConfig": {
            "angularModule": "ckeplugin-changelog",
            "translationKey": "ckeplugin-changelog",
            "navigationItem": false,
            "authentication": {
                "requiresLogin": false
            }
        }
    },
    "author": "Jeroen Valcke",
    "license": "ISC",
    "devDependencies": {
        "@district01/eslint-config": "~1.0.0",
        "gulp": "~3.9.1",
        "gulp-angular-templatecache": "~2.0.0",
        "gulp-clean": "~0.3.2",
        "gulp-replace": "~0.5.4",
        "gulp-zip": "~3.2.0",
        "lodash": "~4.17.5",
        "run-sequence": "~1.2.2"
    }
}